<%= form_with url: "/applications", method: :post, local: true, data: {turbo: false} do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :street_address %>
  <%= f.text_field :street_address %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <%= f.label :zip_code %>
  <%= f.text_field :zip_code %>
  <%= f.label :endorsement %>
  <%= f.text_field :endorsement %>
  <%= f.submit "Submit Application" %>
<% end %>
